{
  "name": "inop",
  "version": "0.3.32",
  "description": "SWC based tool, helps to compile TypeScript modules to Interoperable ESM and CommonJS modules",
  "main": "build/utils.js",
  "type": "module",
  "bin": {
    "inop": "bin/inop.js"
  },
  "scripts": {
    "build": "rm -rf build && swc ./src -d build && rm -rf build/__tests__ && tsc --declaration --emitDeclarationOnly",
    "test": "jest"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
